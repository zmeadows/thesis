@incollection{ZIENKIEWICZ201393,
title = "Chapter 4 - Variational Forms and Finite Element Approximation: 1-D Problems",
editor = "O.C. Zienkiewicz and R.L. Taylor and J.Z. Zhu",
booktitle = "The Finite Element Method: its Basis and Fundamentals (Seventh Edition)",
publisher = "Butterworth-Heinemann",
edition = "Seventh Edition",
address = "Oxford",
pages = "93 - 113",
year = "2013",
isbn = "978-1-85617-633-0",
doi = "https://doi.org/10.1016/B978-1-85617-633-0.00004-6",
url = "http://www.sciencedirect.com/science/article/pii/B9781856176330000046",
author = "O.C. Zienkiewicz and R.L. Taylor and J.Z. Zhu",
keywords = "Variational theorems, Constraints, Penalty methods, Lagrange multiplier methods",
abstract = "This chapter introduces the concept of using a variational principle as the basis for constructing approximate solutions to steady state (static) problems. We show that taking the variation of a principle leads to a set of differential equations, known as Euler equations, and boundary conditions. Thus, the first variation of a variational principle is equivalent to a weak form of the Euler equations. Numerical solutions to a variational theorem may be achieved by direct approximation of the dependent variables involved. For linear differential equations we show when it is possible to deduce an equivalent variational principle from the weak form. The chapter also summarizes the treatment of constraints by a Lagrange multiplier method, as well as by penalty, perturbed Lagrangian and augmented approaches. This chapter includes a toolbox of methods that are useful in later treatment of problems where two or more subdomains are joined together or where constraints such as near incompressibility of elastic bodies or transverse shear constraints in plates are encountered."
}
@article{krongauz1997_a,
author="Krongauz, Y.
and Belytschko, T.",
title={{A Petrov-Galerkin Diffuse Element Method (PG DEM) and its comparison to EFG}},
journal="Computational Mechanics",
year="1997",
month="Mar",
day="01",
volume="19",
number="4",
pages="327--333",
abstract="The diffuse element method (DEM) pioneered{\textvisiblespace}by B. Nayroles, G. Touzot and P. Villon is a meshless method for solving partial differential equations. The original version does not pass the patch test. Here an improved version is proposed. This version is based on a Petrov-Galerkin formulation where test functions are required to meet different conditions than trial functions and is labeled here as PG DEM. It is shown that the original DEM derivative approximation is consistent but not integrable; therefore these functions are not suitable as test functions. A set of conditions on test and trial functions is applied. It is shown that the present Petrov-Galerkin (PG DEM) formulation satisfies the patch test and the numerical results show that it exhibits accuracy and rate of convergence superior to the original DEM, while the ``stiffness matrix'' will be unsymmetric even for self-adjoint problems.",
