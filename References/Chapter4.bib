@article{Puso2008,
author = {Puso, M. A. and Chen, J. S. and Zywicz, E. and Elmer, W.},
title = {Meshfree and finite element nodal integration methods},
journal = {International Journal for Numerical Methods in Engineering},
volume = {74},
number = {3},
pages = {416-446},
keywords = {nodal integration, particle methods, meshfree, explicit time integration},
doi = {10.1002/nme.2181},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/nme.2181},
eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/nme.2181},
abstract = {Abstract Nodal integration can be applied to the Galerkin weak form to yield a particle-type method where stress and material history are located exclusively at the nodes and can be employed when using meshless or finite element shape functions. This particle feature of nodal integration is desirable for large deformation settings because it avoids the remapping or advection of the state variables required in other methods. To a lesser degree, nodal integration can be desirable because it relies on fewer stress point evaluations than most other methods. In this work, aspects regarding stability, consistency, efficiency and explicit time integration are explored within the context of nodal integration. Both small and large deformation numerical examples are provided. Copyright © 2007 John Wiley \& Sons, Ltd.},
year = {2008}
}
@article{KRONGAUZ1997371,
title = "Consistent pseudo-derivatives in meshless methods",
journal = "Computer Methods in Applied Mechanics and Engineering",
volume = "146",
